<table class="player_table">
<tbody>
{{#entries}}
<tr class="{{#is_team}}entrytable_teamrow{{/is_team}}">
<td class="entry_numstr" title="{{created_time_str}}" rowspan="{{entry_rowspan}}">{{numstr}}</td>

{{#is_team}}
<td colspan="4" class="entry_name">{{entry_name}}{{#seeding}} [{{seeding}}]{{/seeding}}</td>
{{/is_team}}
{{^is_team}}
{{#player_slots}}
{{#empty}}
<td colspan="4" class="without_partner">Freimeldung</td>
{{/empty}}
{{^empty}}
<td class="player_name" {{#is_official_id}}title="{{textid}}"{{/is_official_id}}>{{natural_name}}{{#seeding}} [{{seeding}}]{{/seeding}}</td>
<td class="player_league">{{league}}</td><td class="player_winrate">{{winrate_str}}</td>
<td class="player_club{{#player_club_is_special}} player_club_special{{/player_club_is_special}}">{{#player_club}}{{name}}{{/player_club}}</td>
{{/empty}}
{{/player_slots}}
{{/is_team}}

<th rowspan="{{entry_rowspan}}" class="entryrow_buttons">
{{#is_admin}}
<form method="post" action="{{root_path}}entry/{{id}}/edit_dialog" class="inline-form">
{{{csrf_field}}}
<button class="rare_button"><img src="{{icon_path}}edit.svg" title="Meldung bearbeiten" /></button>
</form>

<form method="post" action="{{root_path}}entry/{{id}}/delete" class="inline-form">
{{{csrf_field}}}
<button class="rare_button"><img src="{{icon_path}}delete.svg" title="Meldung lÃ¶schen" /></button>
</form>

<a href="mailto:{{email}}?subject={{#tournament}}{{name}}{{/tournament}}" class="button rare_button {{^email}}email_button_no_email{{/email}}">
<img src="{{icon_path}}email.svg" title="{{email}}" />
</a>

</form>

{{/is_admin}}
</th>
</tr>

{{#is_team}}
{{#players}}
<tr>
<td class="player_name" {{#is_official_id}}title="{{textid}}"{{/is_official_id}}>{{natural_name}}</td>
<td class="player_league">{{league}}</td><td class="player_winrate">{{winrate_str}}</td>
<td class="player_club{{#player_club_is_special}} player_club_special{{/player_club_is_special}}">{{#entry_club}}{{name}}{{/entry_club}}</td>
</tr>
{{/players}}
{{/is_team}}

{{/entries}}
</tbody>
</table>
